#tag Events LocateFile
	#tag Event
		Sub Action()
		  dim f as FolderItem
		  f=GetOpenFolderItem("")
		  if f<> nil then
		    ImportFilePath.text=f.Name
		    Dynamic.ImportFilePath=f.NativePath
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbOK
	#tag Event
		Sub Action()
		  dim n as Integer
		  
		  if rbComma.Value then
		    Dynamic.Seperator=","
		  else
		    Dynamic.Seperator=chr(9)
		  end if
		  
		  Select Case Dynamic.ImportType
		    
		  Case "Participants"
		    Dynamic.ImportParticipantData
		    
		  Case "Transponders"
		    Dynamic.ImportTXData
		    
		  End Select
		  
		  LocateImportFolder.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbCancel
	#tag Event
		Sub Action()
		  LocateImportFolder.Close
		End Sub
	#tag EndEvent
#tag EndEvents
