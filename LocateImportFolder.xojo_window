#tag WindowCode
	#tag Event
		Sub Open()
		  Dynamic.ImportFilePath=""
		  
		  
		  if TargetMacOS then
		    rbComma.Value=false
		    rbTab.Value=true
		  else
		    rbComma.Value=true
		    rbTab.Value=False
		  end if
		End Sub
	#tag EndEvent


